    "name": "boilerplate-code-11ty",
    "version": "0.0.1",
    "description": "11ty Starter - Template Project",
    "browserslist": [
        "defaults"
    ],
    "scripts": {
        "start": "npm run dev",
        "dev": "run-s clean eleventy:dev --print-label",
        "build": "run-s clean eleventy:prod --print-label",
